{
  "provider": {
    "name": "product-service"
  },
  "consumer": {
    "name": "dummy-consumer"
  },
  "interactions": [
    {
      "providerState": "HasProductDetails",
      "description": "a request is made with product id",
      "request": {
        "method": "GET",
        "path": "/products/123"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json;charset=UTF-8"
        },
        "body": {
          "id": "123",
          "name": "Shoe",
          "price": 345.0
        },
        "responseMatchingRules": {
          "$.body.content": {
            "match": "type"
          }
        }
      }
    },
    {
      "providerState": "HasListOfProductDetails",
      "description": "a request is made with product ids",
      "request": {
        "method": "GET",
        "path": "/products",
        "query": {
          "productIds": [
            "123"
          ]
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json;charset=UTF-8"
        },
        "body": [
          {
            "id": "123",
            "name": "Shoe",
            "price": 234.0
          }
        ],
        "responseMatchingRules": {
          "$.body.content": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pact-specification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "2.4.10"
    }
  }
}